#!/home/kinda_rai/PycharmProjects/python-zulip-api-master/zulip-api-py3-venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'zulip','console_scripts','zulip-send'
__requires__ = 'zulip'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('zulip', 'console_scripts', 'zulip-send')()
    )
